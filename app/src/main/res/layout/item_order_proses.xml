<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:gravity="center"
    android:layout_gravity="center"
    android:layout_height="430dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="horizontal">

    <androidx.cardview.widget.CardView
        app:cardCornerRadius="@dimen/_10sdp"
        android:id="@+id/cardView2"
        android:layout_marginEnd="@dimen/_10sdp"
        android:layout_marginStart="@dimen/_10sdp"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_350sdp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <ProgressBar
            android:id="@+id/progressBarProses"
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"/>

        <LinearLayout
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginEnd="@dimen/_10sdp"
            android:layout_marginStart="@dimen/_10sdp"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_marginStart="10dp"
                    android:gravity="center|start"
                    android:text="ID Pengiriman" />

                <TextView
                    android:layout_width="@dimen/_10sdp"
                    android:layout_height="match_parent"
                    android:gravity="center|start"
                    android:text=":" />

                <TextView
                    android:id="@+id/id_pengirimanProses"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center|start"
                    android:text="-"
                    android:textSize="10dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="match_parent"
                    android:layout_marginStart="10dp"
                    android:gravity="center|start"
                    android:text="Provinsi" />

                <TextView
                    android:layout_width="@dimen/_10sdp"
                    android:layout_height="match_parent"
                    android:gravity="center|start"
                    android:text=":" />

                <TextView
                    android:id="@+id/TvProvinsi_orderProses"
                    android:gravity="center|start"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="-"
                    android:textSize="10dp" />
            </LinearLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_marginStart="10dp"
                    android:gravity="center|start"
                    android:text="Kota" />

                <TextView
                    android:layout_width="@dimen/_10sdp"
                    android:layout_height="match_parent"
                    android:gravity="center|start"
                    android:text=":" />

                <TextView
                    android:id="@+id/TvKotaProses"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center|start"
                    android:text="-"
                    android:textSize="10dp" />
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_marginStart="10dp"
                    android:gravity="center|start"
                    android:text="Kecamatan" />

                <TextView
                    android:layout_width="@dimen/_10sdp"
                    android:layout_height="match_parent"
                    android:gravity="center|start"
                    android:text=":" />

                <TextView

                    android:id="@+id/TvDistrikProses"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center|start"
                    android:text="-"
                    android:textSize="10dp" />
            </LinearLayout>





            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_marginStart="10dp"
                    android:gravity="center|start"
                    android:text="Status" />

                <TextView
                    android:layout_width="@dimen/_10sdp"
                    android:layout_height="match_parent"
                    android:gravity="center|start"
                    android:text=":" />

                <TextView
                    android:id="@+id/TvStatusProses"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center|start"
                    android:text="-"
                    android:textSize="10dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_marginStart="10dp"
                    android:gravity="center|start"
                    android:text="Kategori" />

                <TextView
                    android:layout_width="@dimen/_10sdp"
                    android:layout_height="match_parent"
                    android:gravity="center|start"
                    android:text=":" />

                <TextView
                    android:id="@+id/TvKategoriProses"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="-"
                    android:gravity="center|start"
                    android:textSize="10dp" />
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_marginStart="10dp"
                    android:gravity="center|start"
                    android:text="Alamat" />

                <TextView
                    android:layout_width="@dimen/_10sdp"
                    android:layout_height="match_parent"
                    android:gravity="center|start"
                    android:text=":" />

                <TextView
                    android:id="@+id/TvAlamatproses"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center|start"
                    android:text="-"
                    android:textSize="10dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="@dimen/_100sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_marginStart="10dp"
                    android:gravity="center|start"
                    android:text="Gedung" />

                <TextView
                    android:layout_width="@dimen/_10sdp"
                    android:layout_height="match_parent"
                    android:gravity="start|center"
                    android:text=":" />

                <TextView
                    android:id="@+id/TvGedungProses"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center|start"
                    android:text="-"
                    android:textSize="10dp" />
            </LinearLayout>


            <Button
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:id="@+id/btnProsesBarang"
                style="@style/ButtonPrimary"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_45sdp"
                android:layout_marginTop="@dimen/_14sdp"
                android:text="Selesai" />

        </LinearLayout>


    </androidx.cardview.widget.CardView>

</LinearLayout>